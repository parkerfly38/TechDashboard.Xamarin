<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TechDashboard.Views.MainMenu"
             Title="Main Menu">
  <StackLayout Padding="30">
    <Label Text="Main Menu: " FontSize="20" />
    
    <ListView x:Name="lvMainMenu" ItemsSource="{Binding Items}" ItemTapped="lvMainMenu_OnItemTapped" HasUnevenRows="True">      
    </ListView>

  </StackLayout>
</ContentPage>