							<Label Text="{Binding Path=StepText, StringFormat='Step Text: {0}'}"  />
							<Label Text="{Binding Path=SvcAgrmContractCode, StringFormat='SvcAgreement Contract Code: {0}'}" />
							<Label Text="{Binding Path=TransactionDate, StringFormat='Transaction Date: {0}'}" />
							<Label Text="{Binding Path=TransactionDateAsDateTime, StringFormat='Transaction Date(DT): {0}'}" />
							<Label Text="{Binding Path=UnitCost, StringFormat='Unit Cost: {0}'}" />
							<Label Text="{Binding Path=UnitOfMeasure, StringFormat='Unit of Measure: {0}'}" />
