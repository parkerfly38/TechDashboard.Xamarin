							<Label Text="{Binding Path=QuantityScrapped, StringFormat='Quantity Scrapped: {0}' }"/>
							<Label Text="{Binding Path=QuantityUsed, StringFormat='Quantity Used: {0}' }"  />
							<Label Text="{Binding Path=RecordType, StringFormat='Record Type: {0}' }" />
							<Label Text="{Binding Path=ReimburseEmployee, StringFormat='Reimburse Employee: {0}' }" />
							<Label Text="{Binding Path=RemovePart, StringFormat='Remove Part: {0}' }"  />
							<Label Text="{Binding Path=RepairItemCode, StringFormat='Repair Item Code: {0}' }" />
							<Label Text="{Binding Path=ResolutionCode, StringFormat='Resolution Code: {0}' }"/>
							<Label Text="{Binding Path=SalesOrderNo, StringFormat='Sales Order No: {0}' }"  />
