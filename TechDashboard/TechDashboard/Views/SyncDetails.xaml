							<Label Text="{Binding Path=EmployeeNo, StringFormat='Employee No: {0}' }"  />
							<Label Text="{Binding Path=EndTime, StringFormat='End Time: {0}' }" />
							<Label Text="{Binding Path=ExceptionCode, StringFormat='Exception Code: {0}' }"  />
							<Label Text="{Binding Path=FindNo, StringFormat='Find No: {0}' }" />
							<Label Text="{Binding Path=HoursWorked, StringFormat='Hours Worked: {0}' }"  />
							<Label Text="{Binding Path=ImportRecordValid, StringFormat='Import Record Valid: {0}' }"  />
							<Label Text="{Binding Path=ItemCode, StringFormat='Item Code: {0}' }" />
							<Label Text="{Binding Path=ItemCodeDesc, StringFormat='Item Code Desc: {0}' }"   />
							<Label Text="{Binding Path=LastErrorDescription, StringFormat='Last Error Desc: {0}' }"  />
							<Label Text="{Binding Path=LineKey, StringFormat='Line Key: {0}' }"  />
							<Label Text="{Binding Path=LineSeqNo, StringFormat='Line Seq No: {0}' }" />
							<Label Text="{Binding Path=LotSerialNo, StringFormat='Lot Serial No: {0}' }"  />
							<Label Text="{Binding Path=MeterReading, StringFormat='Meter Reading: {0}' }"   />
							<Label Text="{Binding Path=MfgSerialNo, StringFormat='Mfg Serial: {0}' }"   />
							<Label Text="{Binding Path=MobileRecordID, StringFormat='Mobile Record ID: {0}' }"   />
							<Label Text="{Binding Path=Overhead, StringFormat='Overhead: {0}' }"  />
							<Label Text="{Binding Path=Overtime, StringFormat='Overtime: {0}' }"  />
							<Label Text="{Binding Path=PrintPart, StringFormat='Print Part: {0}' }"  />
